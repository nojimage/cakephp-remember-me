{
  "name": "nojimage/cakephp-remember-me",
  "description": "RememberMe authentication adapter plugin for CakePHP 3",
  "type": "cakephp-plugin",
  "license": ["MIT"],
  "require": {
    "php": ">=5.6",
    "ext-json": "*",
    "cakephp/core": "^3.5",
    "cakephp/orm": "^3.5"
  },
  "require-dev": {
    "cakephp/authentication": "^1.0",
    "cakephp/cakephp": "^3.5",
    "cakephp/cakephp-codesniffer": "^3.1",
    "phpunit/phpunit": "^5.7|^6.0"
  },
  "autoload": {
    "psr-4": {
      "RememberMe\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "RememberMe\\Test\\": "tests",
      "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
    }
  },
  "scripts": {
    "cs-check": "phpcs -p --extensions=php --standard=vendor/cakephp/cakephp-codesniffer/CakePHP ./src ./tests",
    "cs-fix": "phpcbf -p --extensions=php --standard=vendor/cakephp/cakephp-codesniffer/CakePHP ./src ./tests"
  },
  "config": {
    "sort-packages": true
  }
}
